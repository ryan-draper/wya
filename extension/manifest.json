{
  "name": "wya",
  "description": "wya: location protection for social media",
  "version": "1.0",
  "manifest_version": 3,
  "host_permissions": [
    "http://localhost:8080/api/v1"
  ],
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/WYA16.png",
      "32": "/images/WYA32.png",
      "48": "/images/WYA48.png",
      "128": "/images/WYA128.png"
    }
  },
  "icons": {
    "16": "/images/WYA16.png",
    "32": "/images/WYA32.png",
    "48": "/images/WYA48.png",
    "128": "/images/WYA128.png"
  }
}